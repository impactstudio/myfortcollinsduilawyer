<div class="container-fluid content-wrapper new-project">
  <div class="row">
    <div class="col-md-10 col-md-offset-1">
      <%= form_for dui do |f| %>

        <div class="form-group">
          <%= f.label :title %>
          <%= f.text_field :title, class: "form-control" %>
        </div>

        <div class="form-group">
          <%= f.label :content %>
          <%= f.text_area :content, class: 'form-control tinymce', :rows => 40, :cols => 100  %>
        </div>

        <%= f.submit "Add DUI Subpage" %>
      <% end %>
      <%= tinymce %>
    </div>
  </div>
</div>
